# API Configuration
NUXT_PUBLIC_API_BASE=http://localhost:8000/api/v1

# Zitadel OAuth Configuration
# Get these from your Zitadel project settings
NUXT_OAUTH_ZITADEL_CLIENT_ID=your_client_id_here
NUXT_OAUTH_ZITADEL_CLIENT_SECRET=your_client_secret_here
NUXT_OAUTH_ZITADEL_SERVER_URL=https://your-zitadel-instance.com

# Session Secret (generate with: openssl rand -base64 32)
NUXT_SESSION_PASSWORD=your_session_secret_here_min_32_chars
